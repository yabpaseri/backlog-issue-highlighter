{
  "name": "backlog-issue-highlighter",
  "description": "Backlogの課題一覧で、閲覧した課題の背景色を変更する機能を追加します。",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "verup": "bun pm version --no-git-tag-version"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/fs-extra": "^11.0.4",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@webext-core/messaging": "^2.3.0",
    "@wxt-dev/auto-icons": "^1.1.0",
    "@wxt-dev/i18n": "^0.2.4",
    "@wxt-dev/module-react": "^1.1.3",
    "defu": "^6.1.4",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "fs-extra": "^11.3.2",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "sharp": "^0.34.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.46.2",
    "wxt": "^0.20.6"
  }
}